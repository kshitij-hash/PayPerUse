          "config": {
            "temperature": 0.2,
            "systemInstruction": "You are a data analysis expert. Analyze the provided data and return insights in JSON format.",
            "maxOutputTokens": 2000
          }
        }
      ],
      "pricing": {
        "amount": "0.05",
        "currency": "USDC"
