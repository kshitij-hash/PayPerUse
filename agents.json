    },
    {
      "id": "agent-002",
      "name": "Image Generation Agent",
      "description": "Generates images from text prompts using Google Gemini",
      "steps": [
        {
          "id": "step-001",
          "serviceId": "generate-image",
          "config": {
            "size": "1024x1024",
            "quality": "standard"
          }
        }
      ],
      "pricing": {
        "amount": "0.10",
        "currency": "USDC"
      }
    },
    {
      "id": "agent-003",
      "name": "Code Assistant Agent",
      "description": "Helps with coding tasks and provides code explanations using Google Gemini",
      "steps": [
        {
          "id": "step-001",
          "serviceId": "gemini-text",
          "config": {
            "temperature": 0.3,
            "systemInstruction": "You are a helpful coding assistant. Provide clear and concise code explanations and solutions.",
            "maxOutputTokens": 1500
          }
        }
      ],
      "pricing": {
        "amount": "0.02",
        "currency": "USDC"
      }
    },
    {
      "id": "agent-004",
      "name": "Multi-Step Translator",
      "description": "Summarizes text and then translates it to French",
      "steps": [
        {
          "id": "step-001",
          "serviceId": "summarize",
          "config": {
            "max_length": 150
          }
        },
        {
          "id": "step-002",
          "serviceId": "translate",
          "config": {
            "target_lang": "fr",
            "preserve_formatting": true
          }
        }
      ],
      "pricing": {
        "amount": "0.10",
        "currency": "USDC"
      }
    },
    {
